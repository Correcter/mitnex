{
    "request": {
        "url": "/uzcard/sms-api",
        "bodyPatterns": [
            {
                "matchesJsonPath": {
                    "expression": "$.method",
                    "equalTo": "fast_p2p.get_pay_code"
                }
            }
        ],
        "method": "POST"
    },
    "response": {
        "status": 200,
        "bodyFileName": "uzcard/sms-api.json",
        "transformers": ["response-template","body-transformer"],
        "headers": {
            "Content-Type": "application/json"
        }
    }
}
