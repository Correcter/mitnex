{
    "request": {
        "urlPath": "/8billing/api/v1/",
        "bodyPatterns": [
            {
                "matchesJsonPath": {
                    "expression": "$.method",
                    "equalTo": "getStatuses"
                }
            }
        ],
        "method": "POST"
    },
    "response": {
        "status": 200,
        "bodyFileName": "8billing/getStatuses.json",
        "transformers": ["response-template"]
    }
}
